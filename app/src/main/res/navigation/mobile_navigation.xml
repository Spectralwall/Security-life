<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigationLogin">

    <fragment
        android:id="@+id/navigationLogin"
        android:name="com.example.securitylife.Fragment.login"
        android:label="Login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_navigationLogin_to_navigationMain"
            app:destination="@id/navigationMain" />
    </fragment>

    <fragment
        android:id="@+id/navigationMain"
        android:name="com.example.securitylife.Fragment.Main_page"
        android:label="Main"
        tools:layout="@layout/fragment_main_page" >
    </fragment>


</navigation>